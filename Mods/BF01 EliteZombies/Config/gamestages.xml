			</gamestage>
			<gamestage stage="400">
				<spawn group="zombieEliteGroupGS400" num="1" maxAlive="1" duration="1"/>
			</gamestage>
			<gamestage stage="600">
				<spawn group="zombieEliteGroupGS600" num="1" maxAlive="1" duration="1"/>
